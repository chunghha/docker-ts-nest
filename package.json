{
  "name": "docker-ts-nest",
  "version": "0.4.2",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && yarn build",
    "start:prod": "cross-env-shell NODE_ENV=prod pm2 start ecosystem.config.js",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "npx compodoc --theme material -p ./tsconfig.json",
    "doc:serve": "npx compodoc -s",
    "commitmsg": "commitlint -e",
    "release": "standard-version --no-verify"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nest-middlewares/response-time": "^6.0.0",
    "@nestjs/bull": "~0.3.1",
    "@nestjs/common": "~7.6.15",
    "@nestjs/core": "~7.6.15",
    "@nestjs/microservices": "~7.6.15",
    "@nestjs/platform-express": "~7.6.15",
    "@nestjs/swagger": "~4.8.0",
    "@nestjs/testing": "~7.6.15",
    "@nestjs/websockets": "~7.6.15",
    "axios": "~0.21.1",
    "bull": "^3.22.10",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "3.23.3",
    "nest-router": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "~6.6.7",
    "supertest": "^6.1.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "@compodoc/compodoc": "^1.1.11",
    "@types/bull": "^3.15.2",
    "@types/hapi__joi": "^17.1.7",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "cross-env": "^7.0.3",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-radar": "^0.2.1",
    "husky": "^7.0.1",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.9",
    "prettier": "~2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "standard-version": "^9.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "~4.3.5"
  },
  "importSort": {
    ".ts": {
      "style": "module",
      "parser": "typescript"
    }
  }
}

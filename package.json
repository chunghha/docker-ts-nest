{
  "name": "docker-ts-nest",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/server.ts",
    "prestart:prod": "tsc",
    "start:watch": "nodemon",
    "start:prod": "node dist/server.js",
    "compile": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run tslint:check",
    "test": "jest --forceExit --coverage --verbose",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:check": "npx tslint-config-prettier-check ./tslint.json",
    "doc": "npx compodoc --theme material -p ./tsconfig.json",
    "doc:serve": "npx compodoc -s",
    "todo": "npx leasot 'src/**/*.ts'",
    "todo-ci": "npx leasot -x --reporter markdown 'src/**/*.ts' > TODO.md"
  },
  "dependencies": {
    "@nest-middlewares/helmet": "~5.1.1",
    "@nest-middlewares/response-time": "~5.1.1",
    "@nestjs/common": "~5.7.2",
    "@nestjs/core": "~5.7.2",
    "@nestjs/microservices": "~5.7.2",
    "@nestjs/swagger": "~2.5.1",
    "@nestjs/testing": "~5.7.2",
    "@nestjs/websockets": "~5.7.2",
    "axios": "^0.18.0",
    "bull": "^3.6.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fastify": "^1.14.1",
    "fastify-formbody": "^2.1.0",
    "fastify-swagger": "^1.2.0",
    "helmet": "^3.15.0",
    "nest-bull": "^0.3.0",
    "nest-router": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "~6.4.0",
    "rxjs-compat": "~6.4.0",
    "supertest": "^3.4.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.8",
    "@types/bull": "^3.5.6",
    "@types/jest": "^24.0.0",
    "@types/node": "~10.12.23",
    "jest": "^24.0.1",
    "leasot": "^7.3.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.3.3"
  }
}

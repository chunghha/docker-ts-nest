{
  "name": "docker-ts-nest",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/server.ts",
    "prestart:prod": "tsc",
    "start:watch": "nodemon",
    "start:prod": "node dist/server.js",
    "compile": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "yarn compile",
    "pretest": "yarn compile",
    "posttest": "yarn tslint:check",
    "test": "jest --forceExit --coverage --verbose",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:check": "npx tslint-config-prettier-check ./tslint.json",
    "doc": "npx compodoc --theme material -p ./tsconfig.json",
    "doc:serve": "npx compodoc -s",
    "todo": "npx leasot 'src/**/*.ts'",
    "todo-ci": "npx leasot -x --reporter markdown 'src/**/*.ts' > TODO.md"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.0",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nest-middlewares/response-time": "^6.0.0",
    "@nestjs/common": "~6.7.2",
    "@nestjs/core": "~6.7.2",
    "@nestjs/microservices": "~6.7.2",
    "@nestjs/platform-express": "~6.7.2",
    "@nestjs/swagger": "~3.1.0",
    "@nestjs/testing": "~6.7.2",
    "@nestjs/websockets": "~6.7.2",
    "axios": "~0.19.0",
    "bull": "^3.11.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "nest-bull": "^0.8.4",
    "nest-router": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "~6.5.3",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@types/bull": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/node": "~12.7.8",
    "jest": "^24.9.0",
    "leasot": "^8.2.0",
    "nodemon": "^1.19.3",
    "prettier": "~1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.6.3"
  }
}

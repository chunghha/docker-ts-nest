{
  "name": "docker-ts-nest",
  "version": "0.6.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --cache --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && yarn build",
    "start:prod": "cross-env-shell NODE_ENV=prod pm2 start ecosystem.config.js",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check:all": "yarn lint && yarn format && yarn test",
    "doc": "npx compodoc --theme material -p ./tsconfig.json",
    "doc:serve": "npx compodoc -s",
    "commitmsg": "commitlint -e",
    "release": "standard-version --no-verify"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nest-middlewares/response-time": "^6.0.0",
    "@nestjs/axios": "0.1.0",
    "@nestjs/bull": "~0.6.0",
    "@nestjs/common": "~9.0.7",
    "@nestjs/core": "~9.0.7",
    "@nestjs/microservices": "~9.0.7",
    "@nestjs/platform-express": "~9.0.7",
    "@nestjs/swagger": "~6.0.4",
    "@nestjs/testing": "~9.0.7",
    "@nestjs/websockets": "~9.0.7",
    "axios": "0.27.2",
    "bull": "^4.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "3.23.3",
    "nest-router": "^1.0.9",
    "nest-winston": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "~7.5.5",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@compodoc/compodoc": "^1.1.19",
    "@types/bull": "^3.15.9",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.18.22",
    "@typescript-eslint/eslint-plugin": "~5.31.0",
    "@typescript-eslint/parser": "~5.31.0",
    "cross-env": "^7.0.3",
    "eslint": "~8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-radar": "^0.2.1",
    "husky": "^8.0.1",
    "import-sort-style-module": "^6.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "~2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "~4.7.4"
  },
  "importSort": {
    ".ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}

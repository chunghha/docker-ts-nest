{
  "name": "docker-ts-nest",
  "version": "0.2.0",
  "scripts": {
    "start": "ts-node src/server.ts",
    "prestart:prod": "tsc",
    "start:watch": "nodemon",
    "start:prod": "node dist/server.js",
    "lint": "tslint src/**/*.ts{,x}",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "test": "jest --forceExit --coverage --verbose"
  },
  "dependencies": {
    "@nest-middlewares/helmet": "^1.1.1",
    "@nest-middlewares/response-time": "^1.1.1",
    "@nestjs/common": "4.6.5",
    "@nestjs/core": "^4.6.5",
    "@nestjs/microservices": "^4.6.5",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.6.1",
    "@nestjs/websockets": "^4.6.5",
    "bull": "^3.3.10",
    "dotenv": "^5.0.1",
    "nest-bull": "^0.2.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~5.5.6"
  },
  "devDependencies": {
    "@types/bull": "^3.3.7",
    "@types/jest": "^22.2.0",
    "@types/node": "^8.9.5",
    "gts": "^0.5.4",
    "jest": "^22.4.2",
    "nodemon": "^1.17.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "~2.7.2"
  }
}

{
  "name": "docker-ts-nest",
  "version": "0.2.0",
  "scripts": {
    "start": "ts-node src/server.ts",
    "prestart:prod": "tsc",
    "start:watch": "nodemon",
    "start:prod": "node dist/server.js",
    "lint": "tslint src/**/*.ts{,x}",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "test": "jest --forceExit --coverage --verbose"
  },
  "dependencies": {
    "@nest-middlewares/helmet": "^1.1.1",
    "@nest-middlewares/response-time": "^1.1.1",
    "@nestjs/common": "4.6.4",
    "@nestjs/core": "^4.6.4",
    "@nestjs/microservices": "^4.6.4",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.6.1",
    "@nestjs/websockets": "^4.5.8",
    "dotenv": "^5.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~5.5.6"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/node": "^8.9.4",
    "gts": "^0.5.3",
    "jest": "^22.4.2",
    "nodemon": "^1.15.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "~2.7.2"
  }
}

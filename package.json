{
  "name": "docker-ts-nest",
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && yarn build",
    "start:prod": "cross-env-shell NODE_ENV=prod pm2 start ecosystem.config.js",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "npx compodoc --theme material -p ./tsconfig.json",
    "doc:serve": "npx compodoc -s",
    "commitmsg": "commitlint -e",
    "release": "standard-version --no-verify"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nest-middlewares/response-time": "^6.0.0",
    "@nestjs/axios": "~0.0.1",
    "@nestjs/bull": "~0.4.0",
    "@nestjs/common": "~8.0.4",
    "@nestjs/core": "~8.0.4",
    "@nestjs/microservices": "~8.0.4",
    "@nestjs/platform-express": "~8.0.4",
    "@nestjs/swagger": "~5.0.8",
    "@nestjs/testing": "~8.0.4",
    "@nestjs/websockets": "~8.0.4",
    "axios": "~0.21.1",
    "bull": "^3.26.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "3.23.3",
    "nest-router": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "~7.2.0",
    "supertest": "^6.1.4",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@compodoc/compodoc": "^1.1.14",
    "@types/bull": "^3.15.2",
    "@types/hapi__joi": "^17.1.7",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "^14.17.6",
    "@typescript-eslint/eslint-plugin": "~4.28.4",
    "@typescript-eslint/parser": "~4.28.4",
    "cross-env": "^7.0.3",
    "eslint": "~7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-radar": "^0.2.1",
    "husky": "^7.0.1",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "nodemon": "^2.0.12",
    "prettier": "~2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "~4.3.5"
  },
  "importSort": {
    ".ts": {
      "style": "module",
      "parser": "typescript"
    }
  }
}

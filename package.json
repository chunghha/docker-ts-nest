{
  "name": "docker-ts-nest",
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && yarn build",
    "start:prod": "cross-env-shell NODE_ENV=prod pm2 start ecosystem.config.js",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "npx compodoc --theme material -p ./tsconfig.json",
    "doc:serve": "npx compodoc -s",
    "commitmsg": "commitlint -e",
    "release": "standard-version --no-verify"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nest-middlewares/response-time": "^6.0.0",
    "@nestjs/bull": "~0.0.3",
    "@nestjs/common": "~6.11.7",
    "@nestjs/core": "~6.11.7",
    "@nestjs/microservices": "~6.11.7",
    "@nestjs/platform-express": "~6.11.7",
    "@nestjs/swagger": "~4.2.11",
    "@nestjs/testing": "~6.11.7",
    "@nestjs/websockets": "~6.11.7",
    "axios": "~0.19.2",
    "bull": "^3.13.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "nest-router": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "~6.5.4",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@compodoc/compodoc": "^1.1.11",
    "@types/bull": "^3.12.0",
    "@types/dotenv": "^8.2.0",
    "@types/hapi__joi": "^16.0.11",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^12.12.28",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.1",
    "prettier": "~1.19.1",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.7.5"
  }
}

{
  "extends": [
      "tslint:latest",
      "tslint-config-prettier"
  ],
  "plugins": [
      "tslint-plugin-prettier"
  ],
  "rulesDirectory": [
      "tslint-plugin-prettier"
  ],
  "rules": {
      "class-name": true,
      "no-var-keyword": true,
      "no-internal-module": true,
      "no-null-keyword": true,
      "prefer-const": true,
      "interface-name": [true, "never-prefix"],
      "jsdoc-format": true,
      "no-console": [false, "log"],
      "no-any": false
  }
}
